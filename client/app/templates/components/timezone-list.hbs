<table class="table">
  <thead>
    <th>Name</th>
    <th>City</th>
    <th></th>
  </thead>
  <tbody>
    {{#each timezones as |timezone|}}
      <tr>
        <td>{{timezone.name}}</td>
        <td>{{timezone.city}}</td>
        <td>
          {{#link-to 'timezones.timezone' timezone}}Show{{/link-to}}
          {{#link-to 'timezones.edit' timezone}}Edit{{/link-to}}
          <a href="#" {{action 'delete' timezone}}>Delete</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
